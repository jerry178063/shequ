<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".view.activity.trade.GoCommissionDetailActivity">

    <include
        android:id="@+id/top"
        layout="@layout/topics_head_sss_layout"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/top"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_title_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/color_303235"
            android:textSize="@dimen/sp_16"
            android:layout_marginTop="@dimen/dp_30"
            android:text=""/>
        <TextView
            android:id="@+id/tv_gauss_ads"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#646566"
            android:textSize="@dimen/sp_14"
            android:layout_marginTop="@dimen/dp_5"
            android:text=""/>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="@dimen/dp_20"
            android:layout_marginBottom="@dimen/dp_20"
            android:background="#F8F8F8"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="质押数量(GPB)"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_ziya_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_303235"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="委托人数"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_weituo_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_303235"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="年化率"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_year_rate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="#F2993D"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="最小委托值"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_min_commision"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_303235"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="总收益"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_get_all_monery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_303235"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_20">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="赎回中"
                android:textColor="#646566"
                android:textSize="@dimen/sp_14"
                android:layout_alignParentLeft="true"/>
            <TextView
                android:id="@+id/tv_shuhui"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="@dimen/sp_16"
                android:textColor="@color/color_303235"
                android:layout_alignParentRight="true"/>

        </RelativeLayout>


    </LinearLayout>

    <TextView
        android:id="@+id/tv_go_commission"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_42"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="@dimen/dp_20"
        android:background="@drawable/assets_detail_transfer_bg"
        android:gravity="center"
        android:text="委托"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16" />


</RelativeLayout>