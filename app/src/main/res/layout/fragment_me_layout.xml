<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@drawable/bg_go_trade">

    </LinearLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/bg_go_trade"
                    android:gravity="center">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="31dp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/dp_170"
                        android:layout_marginTop="@dimen/dp_15"
                        android:text="我的"
                        android:textColor="#fff"
                        android:textSize="@dimen/dp_20"></TextView>

                    <ImageView
                        android:id="@+id/rela_wallet_manager"
                        android:layout_width="@dimen/dp_20"
                        android:layout_height="@dimen/dp_18"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/dp_137"
                        android:layout_marginTop="@dimen/dp_15"
                        android:background="@drawable/qianbao1"></ImageView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_250"
                    android:background="@drawable/bg_go_trade"
                    android:orientation="vertical"></LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_137"
                    android:layout_marginTop="@dimen/dp_40"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/me_chakan"
                        android:layout_width="338dp"
                        android:layout_height="@dimen/dp_60"
                        android:layout_marginLeft="@dimen/dp_13"
                        android:background="@drawable/nfto"
                        android:orientation="vertical">

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/jiao_yiiii"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_77">

                        <include layout="@layout/jiaoyi"></include>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/Ren_zheng"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_62">

                        <ImageView
                            android:layout_width="@dimen/dp_38"
                            android:layout_height="@dimen/dp_38"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:background="@drawable/bianp"></ImageView>

                        <TextView
                            android:layout_width="@dimen/dp_96"
                            android:layout_height="@dimen/dp_20"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_11"
                            android:text="认证数字店铺"
                            android:textColor="#000"
                            android:textSize="@dimen/dp_15"></TextView>

                        <ImageView
                            android:layout_width="@dimen/dp_14"
                            android:layout_height="@dimen/dp_19"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_210"
                            android:background="@drawable/jiantou"></ImageView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/zhiya_me"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_62">

                        <ImageView
                            android:layout_width="@dimen/dp_38"
                            android:layout_height="@dimen/dp_38"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:background="@drawable/meiyuan"></ImageView>

                        <TextView
                            android:layout_width="@dimen/dp_96"
                            android:layout_height="@dimen/dp_20"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_11"
                            android:text="质押保证金"
                            android:textColor="#000"
                            android:textSize="@dimen/dp_15"></TextView>

                        <ImageView
                            android:layout_width="@dimen/dp_14"
                            android:layout_height="@dimen/dp_19"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_210"
                            android:background="@drawable/jiantou"></ImageView>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lin_set"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_62">

                        <ImageView
                            android:layout_width="@dimen/dp_38"
                            android:layout_height="@dimen/dp_38"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:background="@drawable/bianp"></ImageView>

                        <TextView
                            android:layout_width="@dimen/dp_96"
                            android:layout_height="@dimen/dp_20"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_11"
                            android:text="设置"
                            android:textColor="#000"
                            android:textSize="@dimen/dp_15"></TextView>

                        <ImageView
                            android:layout_width="@dimen/dp_14"
                            android:layout_height="@dimen/dp_19"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/dp_210"
                            android:background="@drawable/jiantou"></ImageView>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:layout_width="@dimen/dp_410"
                android:layout_height="@dimen/dp_270"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/dp_50"
                android:background="@drawable/wivff">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_70"
                        android:layout_marginLeft="@dimen/dp_29"
                        android:layout_marginTop="@dimen/dp_34">

                        <ImageView
                            android:layout_width="@dimen/dp_70"
                            android:layout_height="@dimen/dp_70"
                            android:background="@drawable/tou"
                            android:src="@drawable/tou"></ImageView>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/me_name"
                                android:layout_width="24dp"
                                android:layout_height="@dimen/dp_25"
                                android:layout_marginLeft="@dimen/dp_16"
                                android:text="昵 称"
                                android:textColor="#000"
                                android:textSize="@dimen/dp_15"
                                android:textStyle="bold"></TextView>

                            <TextView
                                android:id="@+id/me_qian"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/dp_18"
                                android:layout_marginLeft="@dimen/dp_16"
                                android:layout_marginTop="@dimen/dp_13"
                                android:text="编辑个性签名"
                                android:textColor="#000"
                                android:textSize="@dimen/dp_10"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/dp_120">

                            <ImageView
                                android:id="@+id/layout_bianji"
                                android:layout_width="@dimen/dp_16"
                                android:layout_height="@dimen/dp_16"
                                android:layout_marginLeft="@dimen/dp_40"
                                android:background="@drawable/bianji"></ImageView>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView

                            android:id="@+id/me_unqique"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/dp_18"
                            android:layout_marginLeft="@dimen/dp_30"
                            android:layout_marginTop="@dimen/dp_10"
                            android:textColor="#000"
                            android:textSize="@dimen/dp_10"></TextView>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_30"
                            android:layout_marginTop="@dimen/dp_10"
                            android:src="@drawable/fuzhi"></ImageView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_40">

                        <LinearLayout
                            android:layout_width="@dimen/dp_180"
                            android:layout_height="@dimen/dp_43"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/dp_25"
                                android:layout_marginLeft="@dimen/dp_92"
                                android:text="0"
                                android:textSize="@dimen/dp_15"
                                android:textStyle="bold"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/dp_25"
                                android:layout_marginLeft="@dimen/dp_70"
                                android:text="数字资产"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="@dimen/dp_70"
                            android:layout_height="@dimen/dp_43"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/dp_25"
                                android:layout_marginLeft="@dimen/dp_30"
                                android:text="0"
                                android:textSize="@dimen/dp_15"
                                android:textStyle="bold"></TextView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/dp_25"
                                android:layout_marginLeft="@dimen/dp_9"
                                android:text="我的收藏"></TextView>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="@dimen/dp_70"
                            android:layout_height="@dimen/dp_43"
                            android:layout_marginLeft="@dimen/dp_57">

                            <ImageView
                                android:layout_width="@dimen/dp_43"
                                android:layout_height="@dimen/dp_43"
                                android:background="@drawable/shoukuanma">

                            </ImageView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>
</RelativeLayout>